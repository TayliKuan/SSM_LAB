<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.dept.dao.DeptIN">
	<select id="getOne" resultType="com.dept.model.DeptDoMain">
		select* from dept2 where deptno=#{deptno}
	</select>
	
	<select id="getAll" resultType="com.dept.model.DeptDoMain">
		select* from dept2 order by deptno
 	</select>
</mapper>